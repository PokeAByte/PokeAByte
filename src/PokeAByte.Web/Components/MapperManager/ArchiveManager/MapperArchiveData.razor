@using PokeAByte.Domain
<MudTable Items="Context"
          Dense="true"
          Hover="true"
          Class="mx-4 mb-4">
    <HeaderContent>
        <MudTh>
            Path
        </MudTh>
        <MudTh>
            Display Name
        </MudTh>
        <MudTh>
            Version
        </MudTh>
        <MudTh>
            Date Created
        </MudTh>
        <MudTh>
            Date Updated
        </MudTh>
    </HeaderContent>
    <RowTemplate>
        <MudTd>
            @context.Mapper.Path
        </MudTd>
        <MudTd>
            @context.Mapper.DisplayName
        </MudTd>
        <MudTd>
            @context.Mapper.Version
        </MudTd>
        <MudTd>
            @context.Mapper.DateCreatedUtc
        </MudTd>
        <MudTd>
            @context.Mapper.DateUpdatedUtc
        </MudTd>
    </RowTemplate>
</MudTable>
@code{
    [Parameter] public List<ArchivedMapperDto> Context { get; set; }
}