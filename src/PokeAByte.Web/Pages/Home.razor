@page "/"
@page "/Home"
@using PokeAByte.Web.Services
@using PokeAByte.Web.Services.Navigation
<PageTitle>@PageTitle - Home</PageTitle>
@code
{
    [CascadingParameter] public string? PageTitle { get; set; }
    [Inject] public NavigationService? NavServ { get; set; }
    [Inject] public ILogger<Home> Logger { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        try
        {
            NavServ?.Navigate(NavigationService.Pages.MapperManager);
        }
        catch (Exception e)
        {
            //Logger.LogError(e, "Exception!");
        }
    }
}
