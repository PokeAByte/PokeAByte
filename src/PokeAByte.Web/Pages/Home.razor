@page "/"
@page "/Home"
@using PokeAByte.Web.Services
<PageTitle>@PageTitle - Home</PageTitle>
@code
{
    [CascadingParameter] public string? PageTitle { get; set; }
    [Inject] public NavigationManager? NavMan { get; set; }
    [Inject] public ILogger<Home> Logger { get; set; }

    protected override void OnInitialized()
    {
        base.OnInitialized();
        try
        {
            NavMan?.NavigateTo(AppRoutes.MapperManager);
        }
        catch (Exception e)
        {
            Logger.LogError(e, "Exception!");
        }
    }
}
